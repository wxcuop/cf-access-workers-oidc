{
  "name": "cf-workers-oidc",
  "author": "wxcuop",
  "version": "1.0.0",
  "private": true,
  "module": "./dist/main.mjs",
  "scripts": {
    "build": "npx tsc && node build.js",
    "dev": "miniflare --modules --watch",
    "deploy": "wrangler publish",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:routes": "jest --testPathPattern=routes",
    "test:endpoints": "jest --testPathPattern=endpoints",
    "test:runtime": "vitest run",
    "test:runtime:watch": "vitest",
    "test:jest": "jest --testPathPattern='(?<!runtime)\\.test\\.ts$'",
    "test:all": "npm run test:jest && npm run test:runtime",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "format": "prettier --write '**/*.{js,mjs,css,json,md}'"
  },
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250712.0",
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.7",
    "@vitest/ui": "^3.2.4",
    "esbuild": "^0.13.3",
    "esbuild-plugin-yaml": "^0.0.1",
    "jest": "^29.7.0",
    "prettier": "^1.19.1",
    "rollup": "^2.36.1",
    "rollup-plugin-yaml": "^2.0.0",
    "ts-jest": "^29.4.0",
    "typescript": "^4.4.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.24.3"
  },
  "dependencies": {
    "itty-router": "^2.1.9",
    "jwt-decode": "^3.1.2",
    "miniflare": "^1.3.3",
    "rfc4648": "^1.5.0",
    "uuid": "^8.3.2",
    "worktop": "^0.7.3"
  }
}
